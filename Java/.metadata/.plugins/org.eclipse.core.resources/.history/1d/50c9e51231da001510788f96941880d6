package banco;

class Conta {
	int numero;
	String nome;
	double saldo;
	double limite;
	
	boolean sacar(double valor){
		if(this.saldo < valor){
			return false;
		}
		else{
		this.saldo -= valor;
		return true;
		}
	}
	void depositar(double valor){
		this.saldo += valor; 
	}
	void transfere(Conta destino, double valor){
		this.saldo -= valor;
		destino.saldo += valor;
	}
}
