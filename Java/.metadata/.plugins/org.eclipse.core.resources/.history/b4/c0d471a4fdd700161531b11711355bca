package atvB;

import javax.swing.JOptionPane;

public class q5 {
	
	public static int fatorial(int x){
		if (x <= 1){
			return 1;
		}else{
		int f = x * fatorial(x-1);
		return f;}
	}
	
	public static void main(String[] args) {
		int x = 0;
		while (x <= 40) {
			JOptionPane.showMessageDialog(null, fatorial(x));
		}
	}

}
