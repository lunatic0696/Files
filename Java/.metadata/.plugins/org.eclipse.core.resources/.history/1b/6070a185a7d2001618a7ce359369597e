package infra;

import java.util.Arrays;

public class TestaArray {

	public static void main(String[] args) {
		
		int[] numeros = new int[3];
		numeros[0] = 100;
		numeros[1] = 11;
		numeros[2] = 13;
		
		Arrays.fill(numeros,26);
		
		for(int i = 0; i < 3; i = i + 1){
			System.out.println(numeros[i]);
		}

	}

}
