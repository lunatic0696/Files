package atvB;

import javax.swing.JOptionPane;

public class q5 {
	
	public static int fib(int x){
		int f = fib(x-1);
		return f;
	}
	
	public static void main(String[] args) {
		int x = Integer.valueOf(JOptionPane.showInputDialog("Digite um número"));
		JOptionPane.showMessageDialog(null, fib(x));
	}

}
